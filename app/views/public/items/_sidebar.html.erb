
<div class="col-xs-2">
  <table class="table table-hover table-bordered">
    <thead>
      <tr>
        <th class="text-center"><h5 class="genrekara">ジャンル検索</h5></th>
      </tr>
    </thead>
    <tbody>
      <% genres.each do |genre| %>
        <tr>
          <td><i class="fas fa-caret-right"><%= link_to genre.name, "#" %></i></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
